{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Change cards decks prefix",
  "properties": {
    "Shortcut: Add prefix": {
      "type": "string",
      "pattern": "((Ctrl|Alt|Shift)\\+)?((Ctrl|Alt|Shift)\\+)?((Ctrl|Alt|Shift)\\+)?[a-zA-Z0-9]"
    },
    "Shortcut: Remove prefix": {
      "type": "string",
      "pattern": "((Ctrl|Alt|Shift)\\+)?((Ctrl|Alt|Shift)\\+)?((Ctrl|Alt|Shift)\\+)?[a-zA-Z0-9]"
    },
  }
}
