{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Change cards decks prefix",
  "properties": {
    "Shortcut: Add prefix": {
      "type": "string",
      "pattern": "^((([Cc][Tt][Rr][Ll]|[Aa][Ll][Tt]|[Ss][Hh][Ii][Ff][Tt])\\+){0,3}([a-zA-Z0-9]|F[1-9]|F1[0-2]))?$"
    },
    "Shortcut: Remove prefix": {
      "type": "string",
      "pattern": "^((([Cc][Tt][Rr][Ll]|[Aa][Ll][Tt]|[Ss][Hh][Ii][Ff][Tt])\\+){0,3}([a-zA-Z0-9]|F[1-9]|F1[0-2]))?$"
    },
  }
}
